{
  "firebase": "svinarad",
  "public": "_public",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**",
    "**/bower_components/**"
  ],
  "rewrites": [{
      "source": "/radnik",
      "destination": "/radnik.html"
    }, {
      "source": "/radnik/**",
      "destination": "/radnik.html"
    }, {
      "source": "/",
      "destination": "/index.html"
    }, {
      "source": "/poslodavac",
      "destination": "/poslodavac.html"
    }, {
      "source": "/poslodavac/**",
      "destination": "/poslodavac.html"
    }, {
      "source": "/gallery",
      "destination": "/gallery.html"
    }, {
      "source": "/alija",
      "destination": "/alija.html"
    }, {
      "source": "/alija/**",
      "destination": "/alija.html"
  }]
}
